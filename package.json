{
  "name": "apibusca-saas",
  "version": "2.0.0",
  "description": "Monetized high-performance search API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "prod": "pm2 start src/server.js -i max --name apibusca-saas",
    "stop": "pm2 stop apibusca-saas",
    "reload": "pm2 reload apibusca-saas",
    "logs": "pm2 logs apibusca-saas",
    "setup": "node setup_saas.js",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/style.css",
    "watch:css": "tailwindcss -i ./public/css/input.css -o ./public/css/style.css --watch"
  },
  "keywords": [
    "api",
    "saas",
    "fastify",
    "sqlite"
  ],
  "author": "Gemini Architect",
  "license": "UNLICENSED",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^7.0.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^11.0.0",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "envalid": "^8.1.1",
    "express": "^5.2.1",
    "fastify": "^4.27.0",
    "ioredis": "^5.8.2",
    "lru-cache": "^10.2.2",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "point-of-view": "^6.3.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "pm2": "^5.4.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
